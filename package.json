    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^22.5.4",
    "ejs": "^3.1.10",
    "rollup": "^4.21.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
